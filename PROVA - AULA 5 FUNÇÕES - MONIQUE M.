def maior_numero(a, b, c):
    if a >= b and a >= c:
        return a
    elif b >= a and b >= c:
        return b
    else:
        return c

#Exemplo prático do uso da função:    
print("Vamos descobrir qual o número é o maior?!")
num1 = int(input("Digite o primeiro número: "))
num2 = int(input("Digite o segundo número: "))
num3 = int(input("Digite o terceiro número: "))

# Usa a função definida acima
maior = maior_numero(num1, num2, num3)

# Mostra o resultado
print(f"O maior número entre {num1}, {num2} e {num3} é: {maior}")
